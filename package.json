{
  "name": "chefsire",
  "private": true,
  "version": "0.1.0",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate --schema packages/db/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema packages/db/prisma/schema.prisma",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' packages/db/prisma/seed.ts",
    "postinstall": "prisma generate --schema packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "turbo": "^2.1.3",
    "typescript": "^5.6.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2"
  },
  "packageManager": "npm@10.8.2"
}